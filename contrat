using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace version_1._0
{
    class Contrat
    {
        static int nbContrat;
        private int nbKilométrage;
        private vehicule vehicule;
        private Client client;
        private double CoutTotal;
        private int identifiant;
        public Contrat(int nbKilométrage, vehicule vehicule,Client clients, double CoutTotal)
        {
            this.nbKilométrage = nbKilométrage;
            this.vehicule = vehicule;
            client = clients;
            this.CoutTotal = CoutTotal;
            identifiant = nbContrat;
            nbContrat++;
        }
        public Client Client
        {
            get
            {
                return client;
            }
            set
            {
                client = value;
            }

        }
        public vehicule Vehicule
        {
            get
            {
                return vehicule;
            }
            set
            {
                vehicule = value;
            }
        }
        public int Identifiant
        {
            get
            {
                return identifiant;
            }

        }
        public void CouTotal()
        {

        }


    }
}
